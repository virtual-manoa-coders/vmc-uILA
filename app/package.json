    "bcrypt": "^5.0.1",
    "chart.js": "^2.9.4",
    "classnames": "^2.2.6",
    "core-js": "^3.8.3",
    "graphql": "^14.5.8",
    "meteor-node-stubs": "^1.0.1",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "rc-slider": "^9.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-parallax-tilt": "^1.4.82",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-slick": "^0.28.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^1.3.1",
    "simpl-schema": "^1.10.2",
    "slick-carousel": "^1.8.1",
    "sweetalert": "^2.1.2",
    "uniforms": "^3.0.0",
    "uniforms-bridge-simple-schema-2": "^3.0.0",
    "uniforms-semantic": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-meteor": "^7.2.2",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-utils": "^2.1.0",
    "testcafe": "^1.10.1"
  },
  "scripts": {
    "start": "meteor --no-release-check --exclude-archs web.browser.legacy,web.cordova --settings ../config/settings.development.json",
    "lint": "eslint --quiet --ext .jsx --ext .js ./imports",
    "testcafe-development": "testcafe chrome tests/*.testcafe.js",
    "testcafe": "testcafe chrome:headless tests/*.testcafe.js -q --app \"meteor npm run start\"",
    "testcafe-github": "testcafe chrome:headless tests/*.testcafe.js -q --app \"meteor npm run start\" --app-init-delay 40000 --retry-test-pages",
    "generate-data": "node --harmony ./private/GenerateData.mjs",
    "reset": "meteor reset",
    "presentation" : "npm run reset && npm run generate-data && npm run start"
  }
}
